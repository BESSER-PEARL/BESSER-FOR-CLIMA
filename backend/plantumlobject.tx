Model:
    (objects+=Object | relationships+=Relationship | Comment)*
;

Object:
    'object' '"' className=Class ':' name=ID '"' 'as' alias=ID '{'
        attributes+=Attribute
    '}'
;

Class:
    name=ID
;

Attribute:
    name=ID '=' value=Value
;

Value:
    INT | FLOAT | BOOL | STRING | 'null'
;

Relationship:
    source=ID '-->' target=ID
;

ID: /[\w_]+/;
INT: /-?\d+/;
FLOAT: /-?\d+\.\d+/;
BOOL: 'true' | 'false';
STRING: /".*?"/;
Comment: /\/\/.*$/ | /@startuml/ | /@enduml/;
